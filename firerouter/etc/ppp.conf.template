noauth
refuse-eap
user '#USERNAME#'
password '#PASSWORD#'
nomppe
plugin rp-pppoe.so #INTF#
mru #MRU#
mtu #MTU#
persist
holdoff 10
maxfail 0
usepeerdns
ipcp-accept-remote
ipcp-accept-local
noipdefault
ktune
default-asyncmap
nopcomp
noaccomp
novj
nobsdcomp
nodeflate
lcp-echo-interval 6
lcp-echo-failure 10
lcp-echo-adaptive
unit 0
linkname #LINKNAME#
ifname #LINKNAME#
nodefaultroute
